{% extends 'base.html.twig' %}

{% block body %}
	<div class="form-group container">
		{{ form_start(form) }}
			{{ form_row(form.username) }}
			{{ form_row(form.plainPassword.first) }}
			{{ form_row(form.plainPassword.second) }}

			<button class="btn btn-primary" type="submit">登録</button>
		{{ form_end(form) }}
	</div>
{% endblock %}